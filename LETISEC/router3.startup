ifconfig eth0 14.4.3.254/24 up
ifconfig eth1 6.6.6.2/24 up
ip route add 0.0.0.0/0 via 6.6.6.1


rm -rf /etc/openvpn
mv openvpn_client /etc/openvpn/

mkdir -p /dev/net
mknod /dev/net/tun c 10 200
chmod 600 /dev/net/tun
/etc/init.d/openvpn restart
openvpn client.conf &

iptables -A FORWARD -m state --state ESTABLISHED -j ACCEPT
iptables -A FORWARD -i eth1 -m iprange --src-range 14.4.3.0-14.4.3.255 -j DROP
iptables -A FORWARD -p tcp --dport 22 -m state --state NEW -j ACCEPT
iptables -A FORWARD -p tcp --dport 80 -m state --state NEW -j ACCEPT
iptables -A FORWARD -p udp --dport 33434:33534 -m state --state NEW -j ACCEPT
iptables -A FORWARD -p icmp -j ACCEPT
iptables -A FORWARD -j DROP